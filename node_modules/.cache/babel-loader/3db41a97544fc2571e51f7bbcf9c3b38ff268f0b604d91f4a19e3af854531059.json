/VTitle\";\nimport BlockTitle from \"@/components/text/BlockTitle\";\nexport default {\n  components: {BlockTitle, VTitle, ProductViewImage, VButton},\n  data() {\n    return {\n      productId: '3687273',\n      product: {},\n      params: {\n        'Белки': 5,\n        \n      }\n    }\n  },\n  methods: {\n    async getProduct(productId) {\n      try {\n        const response = await axios.get('https://rskrf.ru/rest/1/product/'+productId);\n        this.product = response.data.response;\n      }\n      catch (e) {\n        console.error(e);\n      }\n    },\n    getNutritionalValue() {\n      const nutritionalValue = this.product.product_info.find((item) => item.name === \"Дополнительная информация\");\n      console.log(nutritionalValue.info);\n    }\n  },\n  async mounted() {\n    await this.getProduct(this.productId);\n    console.log(this.product);\n    this.getNutritionalValue();\n  }\n}\n</script>\n\n<style scoped>\n.block{\n  margin-top: 20px;\n}\n</style>"],"mappings":"AAWA,OAAOA,OAAM,MAAO,8BAA8B;AAClD,OAAOC,KAAI,MAAO,OAAM;AACxB,OAAOC,gBAAe,MAAO,2BAA2B;AACxD,OAAOC,MAAK,MAAO,0BAA0B;AAC7C,OAAOC,UAAS,MAAO,8BAA8B;AACrD,eAAe;EACbC,UAAU,EAAE;IAACD,UAAU;IAAED,MAAM;IAAED,gBAAgB;IAAEF;EAAO,CAAC;EAC3DM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,SAAS;MACpBC,OAAO,EAAE,CAAC,CAAC;MACXC,MAAM,EAAE;QACN,OAAO,EAAE;MAEX;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAACJ,SAAS,EAAE;MAC1B,IAAI;QACF,MAAMK,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAG,CAAC,kCAAkC,GAACN,SAAS,CAAC;QAC9E,IAAI,CAACC,OAAM,GAAII,QAAQ,CAACN,IAAI,CAACM,QAAQ;MACvC,EACA,OAAOE,CAAC,EAAE;QACRC,OAAO,CAACC,KAAK,CAACF,CAAC,CAAC;MAClB;IACF,CAAC;IACDG,mBAAmBA,CAAA,EAAG;MACpB,MAAMC,gBAAe,GAAI,IAAI,CAACV,OAAO,CAACW,YAAY,CAACC,IAAI,CAAEC,IAAI,IAAKA,IAAI,CAACC,IAAG,KAAM,2BAA2B,CAAC;MAC5GP,OAAO,CAACQ,GAAG,CAACL,gBAAgB,CAACM,IAAI,CAAC;IACpC;EACF,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACd,UAAU,CAAC,IAAI,CAACJ,SAAS,CAAC;IACrCQ,OAAO,CAACQ,GAAG,CAAC,IAAI,CAACf,OAAO,CAAC;IACzB,IAAI,CAACS,mBAAmB,CAAC,CAAC;EAC5B;AACF"},"metada